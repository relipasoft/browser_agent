{
  "manifest_version": 3,
  "name": "Relipa Browser Agent",
  "version": "1.0",
  "description": "Extension có thể điều khiển browser để hỗ trợ người dùng giải quyết task cụ thểthể",
  "side_panel": {
    "default_path": "sidepanel.html",
    "open_panel_text": "Chat"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "sidePanel"
  ],
  "host_permissions": [
    "http://localhost:8888/*"
  ]
}
